{
  "name": "furqansiddiqui/ethereum-php",
  "description": "A modern, dependency-light Ethereum library powered by a clean cryptographic core",
  "type": "library",
  "homepage": "https://github.com/furqansiddiqui/ethereum-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Furqan A. Siddiqui",
      "email": "hello@furqansiddiqui.com",
      "homepage": "https://www.furqansiddiqui.com",
      "role": "Author"
    }
  ],
  "require": {
    "php-64bit": "^8.5",
    "ext-openssl": "*",
    "ext-gmp": "*",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "charcoal-dev/http-client": "^0.2.13",
    "furqansiddiqui/blockchain-core-php": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "FurqanSiddiqui\\Ethereum\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FurqanSiddiqui\\Ethereum\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
